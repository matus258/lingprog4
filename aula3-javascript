<!DOCTYPE html>
<html>
<head>
	<title></title>
	<script type="text/javascript">
		function ProgramaJS()
		{
			let novoH2, numeroRep, i;
			numeroRep = Number(c1.value);
			i=0;
			while(i<numeroRep)
			{
				novoH2 = document.createElement("H2");
				novoH2.innerHTML = "FATEPG";
				document.body.appendChild(novoH2);
				i++;
			}
		}
	</script>
</head>
<body>
	<input type="number" id="c1">
	<br>
	<button onclick="ProgramaJS()">Gerar </button>
</body>
</html>
